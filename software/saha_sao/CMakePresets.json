{
    "version": 2,
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 20
    },
    "configurePresets": [
        {
            "name": "build/saha_sao",
            "displayName": "Build for nRF52 DK nRF52832",
            "generator": "Ninja",
            "binaryDir": "${sourceDir}/build/saha_sao",
            "cacheVariables": {
                "BOARD": "nrf52dk/nrf52832",
                "CONF_FILE": "prj.conf",
                "DTC_OVERLAY_FILE": "nrf52dk_nrf52832.overlay",
                "CONFIG_SPEED_OPTIMIZATIONS": true
            }
        }
    ]
}